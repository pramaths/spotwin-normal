{
  "expo": {
    "name": "spotwin",
    "slug": "spotwin",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon":{
        "light":"./assets/images/apple.png",
        "dark":"./assets/images/apple.png",
        "tinted":"./assets/images/apple.png"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.pramaths.spotwin",
      "buildNumber": "6",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pramaths.spotwin",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],[
        "expo-notifications",
        {
          "sounds": ["./assets/sound.wav"]
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow Spotwin to access your Face ID biometric data."
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "pramaths",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "14da54a5-7894-4e8e-9a8d-b8a54de22b9d"
      }
    }
  }
}
